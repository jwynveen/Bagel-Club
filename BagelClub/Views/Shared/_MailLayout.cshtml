@{ var appHelper = new Laughlin.Common.AppHelper(); }
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>Bagel Club</title>
</head>
<body>
<div style="margin:0; padding:0; width:100%; font-family:Tahoma, Verdana, Sans-serif; font-size:10px; text-align:center; color:#999;">
<p>Having trouble reading this email? Check the <a href="@appHelper.BaseUrl" style="color:#999">online version here</a>.</p>
<table width="620" border="0" align="center" cellpadding="0" cellspacing="0" style="margin:0 auto; padding:0; font-family:Tahoma, Verdana, Sans-serif; font-size:14px; color:#333;">
	<thead>
		<tr>
			<th>
				<h1 style="font-size:18px;">Laughlin Constable Bagel Club</h1>
			</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td style="padding:10px 20px;">
				@RenderBody()
			</td>
		</tr>
	</tbody>
	<tfoot>
        <tr>
            <td style="padding:10px 20px;">
                Sincerely,<br />
                Travis K. Jansen<br />
                Bagel Club President <br />
                <i>"I'm not only the president, I'm also a member..."</i><br /><br />
            </td>
        </tr>
		<tr>
			<td bgcolor="#CCCCCC" style="padding:5px 20px; font-size:12px; color:#666;">
				<p>This email brought to you by the <a href="@appHelper.BaseUrl">Laughlin Constable Bagel Club</a>.</p>
			</td>
		</tr>
	</tfoot>
</table>
</div>
</body>
</html>
